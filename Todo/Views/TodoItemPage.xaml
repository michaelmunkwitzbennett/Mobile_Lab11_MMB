<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Todo.TodoItemPage" Title="{Binding Joke}">
    <StackLayout Margin="20" VerticalOptions="StartAndExpand">
        <Label Text="Joke" />
        <Editor Text="{Binding Joke}" WidthRequest="200" HeightRequest="200" />

        <Button Text="Save" Clicked="OnSaveClicked" />
    </StackLayout>
</ContentPage>